<idea-plugin>
    <extensions defaultExtensionNs="com.intellij">
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved procedure"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RsUnresolvedProcedureInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unresolved command"
                         enabledByDefault="true"
                         level="ERROR"
                         implementationClass="io.runescript.plugin.ide.inspections.RsUnresolvedCommandInspection"/>
        <localInspection language="RuneScript"
                         groupName="RuneScript"
                         displayName="Unreachable code"
                         enabledByDefault="true"
                         level="WARNING"
                         implementationClass="io.runescript.plugin.ide.inspections.RsUnreachableCodeInspection"/>
    </extensions>
</idea-plugin>